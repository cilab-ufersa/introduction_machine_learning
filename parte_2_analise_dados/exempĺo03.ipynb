        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "dados = pd.read_csv('programacao2023.csv')\n",
        "\n",
        "subconjunto1 = dados.loc[dados['Media'] < 6]\n",
        "\n",
        "subconjunto2 = dados.loc[dados['Media'] > 9]\n",
        "\n",
        "subconjunto3 = pd.concat([subconjunto1, subconjunto2], axis=0)\n",
        "\n",
        "subconjunto3\n"
